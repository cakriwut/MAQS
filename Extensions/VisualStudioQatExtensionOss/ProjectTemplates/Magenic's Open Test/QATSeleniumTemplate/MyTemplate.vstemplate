<VSTemplate Version="3.0.0" Type="ProjectGroup"
    xmlns="http://schemas.microsoft.com/developer/vstemplate/2005">
    <TemplateData>
        <Name>Maqs Framework - Selenium</Name>
        <Description>Magenic's automation quick start framework for selenium testing</Description>
        <Icon>QATTest.ico</Icon>
        <ProjectType>CSharp</ProjectType>
		<ProjectSubType>Magenic Test</ProjectSubType>
    </TemplateData>
    <TemplateContent>
        <ProjectCollection>
                <ProjectTemplateLink ProjectName="PageModel">
                    PageModel\MyTemplate.vstemplate
                </ProjectTemplateLink>
                <ProjectTemplateLink ProjectName="Tests">
                    Tests\MyTemplate.vstemplate
                </ProjectTemplateLink>
        </ProjectCollection>
    </TemplateContent>
</VSTemplate>