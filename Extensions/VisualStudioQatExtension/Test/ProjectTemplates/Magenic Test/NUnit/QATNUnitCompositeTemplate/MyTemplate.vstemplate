<VSTemplate Version="3.0.0" Type="ProjectGroup"
    xmlns="http://schemas.microsoft.com/developer/vstemplate/2005">
    <TemplateData>
        <Name>Maqs Framework - NUnit - Composite</Name>
        <Description>Magenic's automation quick start framework for compsite testing using NUnit</Description>
        <Icon>QATTest.ico</Icon>
        <ProjectType>CSharp</ProjectType>
		<ProjectSubType>Magenic Test</ProjectSubType>
    </TemplateData>
    <TemplateContent>
        <ProjectCollection>
                <ProjectTemplateLink ProjectName="Models">
                    Models\MyTemplate.vstemplate
                </ProjectTemplateLink>
                <ProjectTemplateLink ProjectName="Tests">
                    Tests\MyTemplate.vstemplate
                </ProjectTemplateLink>
        </ProjectCollection>
    </TemplateContent>
</VSTemplate>