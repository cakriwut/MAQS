<VSTemplate Version="3.0.0" xmlns="http://schemas.microsoft.com/developer/vstemplate/2005" Type="Project">
  <TemplateData>
    <Name>Tests</Name>
    <Description>Appium test template</Description>
    <ProjectType>CSharp</ProjectType>
    <ProjectSubType>Magenic Test</ProjectSubType>
    <SortOrder>1000</SortOrder>
    <CreateNewFolder>true</CreateNewFolder>
    <DefaultName>Tests</DefaultName>
    <ProvideDefaultName>true</ProvideDefaultName>
    <LocationField>Enabled</LocationField>
    <EnableLocationBrowseButton>true</EnableLocationBrowseButton>
    <Icon>__TemplateIcon.ico</Icon>
	<Hidden>true</Hidden> 
	<CreateInPlace>true</CreateInPlace>
  </TemplateData>
  <TemplateContent>
    <Project TargetFileName="Tests.csproj" File="Tests.csproj" ReplaceParameters="true">
      <ProjectItem ReplaceParameters="true" TargetFileName="appsettings.json">appsettings.json</ProjectItem>
      <ProjectItem ReplaceParameters="true" TargetFileName="AppiumTestsVSUnit.cs">AppiumTestsVSUnit.cs</ProjectItem>
      <ProjectItem ReplaceParameters="true" TargetFileName="AppiumTestsNUnit.cs">AppiumTestsNUnit.cs</ProjectItem>
    </Project>
  </TemplateContent>
</VSTemplate>